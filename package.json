{
  "name": "poker-hand",
  "version": "1.0.0",
  "description": "Parse for poker hands",
  "main": "src/index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "node test/test-card.js",
    "build-umd": "rollup ./src/index.js --file build/poker-hand.js --format umd --name \"PokerHand\"",
    "build-iife": "rollup ./src/index.js --file build/poker-hand.mjs --format iife --name \"PokerHand\"",
    "watch-cjs": "rollup ./src/index.js --watch --file build/poker-hand.mjs --format cjs",
    "build-cjs": "rollup ./src/index.js --file build/poker-hand.mjs --format cjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "simple-statistics": "^6.1.1"
  }
}
